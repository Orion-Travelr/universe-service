{
  "name": "travelr-js.services.universe",
  "version": "1.0.0",
  "description": "Port of LAMP application to JS with NEST",
  "author": "Jake Toolson",
  "license": "proprietary",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaketoolson/travelr-js"
  },
  "scripts": {
    "build:dev": "npm run clean && tsc -p tsconfig.json",
    "build": "npm run clean && tsc -p tsconfig.release.json",
    "clean": "rimraf dist",
    "copy:assets": "node copyAssets.js",
    "serve:dev": "NODE_ENV=development nodemon --watch src --ext ts,tsx,js --exec ts-node --delay 3 src/server.ts",
    "serve": "node dist/server.js universe",
    "start": "npm run serve",
    "test": "lab --assert code --coverage --leaks"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.2",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-express": "^10.3.2",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "@mikro-orm/core": "^6.3.8",
    "@mikro-orm/reflection": "^6.3.8",
    "@mikro-orm/sqlite": "^6.3.8",
    "@nestjs/jwt": "^10.2.0",
    "axios": "^1.7.7",
    "helmet": "^7.1.0",
    "dotenv": "^9.0.2",
    "jsonapi-serializer": "^3.6.9",
    "lodash": "^4.17.21",
    "nest-winston": "^1.9.7",
    "query-string": "^6.13.7",
    "uuid": "8.3.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.1",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "@mikro-orm/cli": "^6.3.8",
    "@types/bluebird": "^3.5.33",
    "@types/cors": "^2.8.6",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.14.7",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "code": "^5.2.4",
    "faker": "^4.1.0",
    "file-loader": "6.2.0",
    "lab": "^18.0.2",
    "nodemon": "2.0.6",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "tsc-watch": "^5.0.3",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">= 8",
    "npm": "^5.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules"
    ],
    "delay": 2500
  }
